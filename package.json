    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.1",
    "vuetify": "^3.10.4"
  },
  "devDependencies": {
    "@feature-sliced/eslint-config": "^0.1.1",
    "@nuxt/eslint-config": "^1.9.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
